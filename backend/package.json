{
  "name": "jogolimpo-backend",
  "version": "1.0.0",
  "description": "Jogo Limpo - Backend API",
  "main": "dist/server.js",
  "scripts": {
    "predev": "bash scripts/prisma-with-env.sh npx prisma generate",
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "pretest": "bash scripts/prisma-with-env.sh npx prisma generate",
    "db:generate": "bash scripts/prisma-with-env.sh npx prisma generate",
    "db:push": "bash scripts/prisma-with-env.sh npx prisma db push",
    "db:studio": "prisma studio",
    "vercel-build": "bash scripts/prisma-with-env.sh sh -c 'npx prisma generate && npx prisma db push && tsc'",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@fastify/jwt": "^10.0.0",
    "@prisma/client": "^6.3.1",
    "bcryptjs": "^3.0.3",
    "fastify": "^5.2.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22.13.1",
    "@vercel/node": "^5.6.2",
    "@vitest/coverage-v8": "^4.0.18",
    "prisma": "^6.3.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^4.0.18"
  }
}
